<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.text.*?>
<FlowPane userData="Temperature conversion" fx:controller="view.TemperatureViewController" maxHeight="-Infinity"
          maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="230.0" prefWidth="475.0"
          xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="173.0" prefWidth="480.0">
            <children>
                <Button fx:id="buttonToCelcius" onAction="#toCelsius" layoutX="219.0" layoutY="41.0"
                        mnemonicParsing="false" prefHeight="30.0" prefWidth="106.0" text="To celcius">
                    <font>
                        <Font size="14.0"/>
                    </font>
                </Button>
                <Button fx:id="buttonToFahrenheit" onAction="#toFahrenheit" layoutX="220.0" layoutY="90.0"
                        mnemonicParsing="false" text="To Fahrenheit">
                    <font>
                        <Font size="14.0"/>
                    </font>
                </Button>
                <Label fx:id="labelTimer" layoutX="380.0" layoutY="71.0" text="00:00:00">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Label>
                <Label layoutX="68.0" layoutY="48.0" text="Temperature:">
                    <font>
                        <Font size="14.0"/>
                    </font>
                </Label>
                <TextField fx:id="textInput" layoutX="61.0" layoutY="69.0" prefHeight="25.0" prefWidth="97.0"/>
                <Label fx:id="labelOutput" layoutX="68.0" layoutY="142.0" text="Output: " textFill="#f80505">
                    <font>
                        <Font size="14.0"/>
                    </font>
                </Label>
            </children>
        </AnchorPane>

    </children>
</FlowPane>
